{
	"compilerOptions": {
		"strict": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2015",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"sourceMap": true,
		"suppressImplicitAnyIndexErrors": true,
		"target": "es5",
		"types": [
			"jquery",
			"bootstrap",
			"node"
		]
	},
	"exclude": [
		"node_modules/*"
	]
}
